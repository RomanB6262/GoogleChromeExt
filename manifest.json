{
  "manifest_version": 3,
  "name": "PhishGuard",
  "version": "1.0.3",
  "description": "Simple anti-phishing scanner.",
  "icons": {
    "128": "Icons/icon128.png",
    "48": "Icons/icon48.png",
    "16": "Icons/icon16.png"
  },
  "permissions": [
    "tabs", 
    "activeTab", 
    "storage", 
    "notifications"  
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}